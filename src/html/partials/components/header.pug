// Header
+b.HEADER.header
  .container
    .row
      .col-xs-12.col-md-8
        +e.UL.list
          +e.LI.item
            +e.P.text Есть вопросы?
          +e.LI.item
            i.fa.fa-reply
            +e.A.email(href="mailto:support@ sres.com ") support@ sres.com
          +e.LI.item
            i.fa.fa-map-marker
            +e.P.adress ул. Байкеров, Москва
          +e.LI.item
            i.fa.fa-phone
            +e.A.phone(href="tel:+78122229899") +7 812 222 98-99
      .col-xs-12.col-md-4
        +e.UL.socials
          +e.LI.socials-item
            a(href="twitter.com" target="_blank")
              i.fa.fa-twitter
          +e.LI.socials-item
            a(href="facbook.com" target="_blank")
              i.fa.fa-facebook-official
          +e.LI.socials-item
            a(href="instagram.com" target="_blank")
              i.fa.fa-instagram
          +e.LI.socials-item
            a(href="ru.linkedin.com" target="_blank")
              i.fa.fa-linkedin
          +e.LI.socials-item
            a(href="vk.com" target="_blank")
              i.fa.fa-vk


